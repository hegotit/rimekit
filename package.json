{
  "name": "rimekit",
  "version": "0.1.0",
  "description": "Rime Kit: An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev2": "electron-vite dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "build2": "npm run typecheck && electron-vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix --ignore-path .gitignore",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "format": "prettier --write src/",
    "format2": "prettier --write .",
    "eslint-config-output": "eslint --print-config eslint.config.js > eslint-config-output.json"
  },
  "author": {
    "name": "hegotit",
    "email": "hegotit18@outlook.com"
  },
  "homepage": "https://github.com/hegotit/rimekit",
  "repository": {
    "type": "git",
    "url": "git://github.com/hegotit/rimekit.git"
  },
  "bugs": {
    "url": "https://github.com/hegotit/rimekit/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/lotem/rimekit/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 20.10.0"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "diff": "^5.2.0",
    "element-plus": "^2.8.0",
    "html-entities": "^2.5.2",
    "less": "^4.2.0",
    "path": "^0.12.7",
    "pinia": "^2.2.1",
    "trie-search": "^2.0.0",
    "vue": "^3.4.37",
    "vue-router": "^4.4.3",
    "winreg2": "^1.3.3",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/diff": "^5.2.1",
    "@types/node": "^22.2.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.27.0",
    "prettier": "^3.3.3",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.4.0",
    "vue-tsc": "^2.0.29"
  },
  "keywords": []
}
